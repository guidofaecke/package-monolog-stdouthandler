{
    "name": "maxime-pasquier/monolog-stdout-handler",
    "description": "A handler for Monolog that sends messages to stdout (with color).",
    "keywords": ["monolog", "log", "stdout", "color"],
    "homepage": "https://bitbucket.org/maxime-pasquier/package-monolog-stdouthandler",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Maxime Pasquier",
            "homepage": "https://bitbucket.org/maxime-pasquier/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "monolog/monolog": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {"Monolog": "src/"}
    }
}
